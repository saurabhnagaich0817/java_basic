import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sum = 0,x = 0;
        while(n>0){
            int rem = n%10;
            sum+=rem*Math.pow(2,x);
            x++;
            n/=10;
        }
    System.out.print(sum);

    }
}

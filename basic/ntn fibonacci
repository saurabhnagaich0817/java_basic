import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(); //index of no
        int a = 0;
        int b = 1;
        int i = 0;
        while(i<n){
        int c = a+b;
            a = b;
            b = c;
i++;
        }
    System.out.println(a);
    }
}

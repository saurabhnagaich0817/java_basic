import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int s = sc.nextInt();
       
        while(s!=0){
            int rem = n%s;
            n = s;
            s = rem;

        }
     
         System.out.println(n);
    }
}
